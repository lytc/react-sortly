{
  "name": "react-tree",
  "version": "1.0.0",
  "description": "React Tree",
  "main": "index.js",
  "scripts": {
    "dev": "node ./server.js",
    "flow": "flow",
    "lint": "$(npm bin)/eslint src site",
    "build": "better-npm-run build:lib && better-npm-run build:dist && better-npm-run build:finally",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "betterScripts": {
    "build:lib": {
      "command": "rm -rf lib && $(npm bin)/babel src --source-maps --out-dir lib",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:dist": {
      "command": "rm -rf dist && $(npm bin)/webpack -p --progress --config webpack.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:finally": "mv dist/app.js docs/ && mv dist/app.js.map docs/"
  },
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=15.0.0",
    "prop-types": "*",
    "immutability-helper": "*",
    "classnames": "*",
    "react-dnd": "^2.5.4",
    "react-dnd-html5-backend": "^2.5.4",
    "lodash.clonedeep": "^4.5.0",
    "lodash.throttle": "^4.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-flow-runtime": "^0.15.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.12",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "better-npm-run": "^0.1.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.40.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "express": "^4.16.2",
    "faker": "^4.1.0",
    "flow-bin": "^0.61.0",
    "flow-runtime": "^0.16.0",
    "highlight.js": "^9.12.0",
    "lodash.debounce": "^4.0.8",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.0.9",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-json-view": "^1.14.0",
    "react-router-dom": "^4.2.2",
    "sass-loader": "^6.0.6",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.1",
    "webpack-hmr": "^1.0.2"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "immutability-helper": "^2.6.2",
    "lodash.clonedeep": "^4.5.0",
    "lodash.throttle": "^4.1.1",
    "prop-types": "^15.6.0",
    "react-dnd": "^2.5.4",
    "react-dnd-html5-backend": "^2.5.4"
  }
}
